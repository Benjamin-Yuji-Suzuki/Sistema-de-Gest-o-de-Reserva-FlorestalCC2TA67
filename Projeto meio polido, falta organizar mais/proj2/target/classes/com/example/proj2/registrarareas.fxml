<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="533.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.proj2.RegistrarareasController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="553.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="24.0" layoutY="24.0" prefHeight="45.0" prefWidth="315.0" text="Digite as informações da área">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
            <Label layoutX="108.0" layoutY="98.0" text="Nome da área" />


            <Button fx:id="confirmar1" layoutX="294.0" layoutY="328.0" mnemonicParsing="false" onAction="#confirmar" text="Confirmar" />
            <TextField fx:id="nomedaarea" layoutX="187.0" layoutY="88.0" prefHeight="37.0" prefWidth="249.0" />
            <TextField fx:id="tamanhoemhectares" layoutX="187.0" layoutY="247.0" prefHeight="37.0" prefWidth="249.0" />
            <Label layoutX="65.0" layoutY="257.0" text="Tamanho em Hectares" />
            <Button fx:id="mapa" layoutX="67.0" layoutY="166.0" mnemonicParsing="false" onAction="#definirLatitudeLongitude" text="Definir latitude e longitude" />
            <TextField fx:id="latitude" layoutX="267.0" layoutY="190.0" prefHeight="17.0" prefWidth="50.0" text="vazio" editable="false"/>
            <TextField fx:id="longitude" layoutX="368.0" layoutY="190.0" prefHeight="17.0" prefWidth="50.0" text="vazio" editable="false"/>
            <Label layoutX="267.0" layoutY="151.0" prefHeight="25.0" prefWidth="57.0" text="Latitude">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="368.0" layoutY="151.0" prefHeight="25.0" prefWidth="69.0" text="Longitude">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
